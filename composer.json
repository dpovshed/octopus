{
  "name": "octopoda/octopus",
  "description": "PHP Sitemap crawler",
  "license": "MIT",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-simplexml": "*",
    "clue/reactphp-flux": "^1.3",
    "psr/log": "^1.1",
    "react/http": "^1.2",
    "symfony/console": "^5.2",
    "teapot/status-code": "^1.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "@stable",
    "phpstan/phpstan": "@stable",
    "phpunit/phpunit": "^8.0",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Octopus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Octopus\\Test\\": "tests/"
    }
  }
}
